rigler.org () 
{ 
    ssh -i ~/purple/.ssh/id_rsa rigler@rigler.org $*
}

alp.rigler.org () 
{ 
    ssh -i ~/purple/.ssh/id_rsa rigler@rigler.org alp/$*
}
clone.alp.rigler.org () 
{
    GITHUB=https://github.com/johnrigler/alp
    ssh -i ~/purple/.ssh/id_rsa rigler@rigler.org "git clone $GITHUB"
}

FIONA=192.168.1.156
