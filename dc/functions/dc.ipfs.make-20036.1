dc.ipfs.make () 
{ 
    AMT="0.00002311";
    IPFS=$(basename $2);
    FIRST=$(echo $IPFS | cut -c 1-23);
    LAST=$(echo $IPFS | cut -c 24-);
    echo $FIRST $LAST;
    echo "$1-cli sendmany \"\" \"{\\";
    ONE=$(un DCx "$3");
    TWO=$(un "DDx$FIRST");
    THREE=$(un "DEx$LAST");
    echo \\\"$ONE\\\"":$AMT,\\";
    echo \\\"$TWO\\\"":$AMT,\\";
    echo \\\"$THREE\\\"":$AMT}\""
}
