a.mod () 
{ 
    __T=a.mod;
    : : Set ALP Module, takes one variable;
    export _ALP_MOD="$1";
    if [[ $_ALP_MOD != 'alp' ]]; then
        _FILE=$_ALP_DIR/$_ALP_MOD.bash;
        touch $_FILE;
        source $_FILE;
        function _a.mod () 
        { 
            NAME="$1.$2";
            - $NAME;
            - "$NAME () { __T=$NAME; :: $3 ; $4; }" > $$;
            source $$;
            rm $$;
            declare -f $NAME
        };
        CMD="declare -F | grep 'declare -f $_ALP_MOD' | cut -c 12-";
        _a.mod $_ALP_MOD l "List module functions" "$CMD";
    fi
}
