ipfs.make () 
{ 
    : arg1 currency;
    : arg2 CID;
    : arg3 name;
    : arg4 medium;
    IPFS=$(basename $2);
    FIRST=$(echo $IPFS | cut -c 1-23);
    LAST=$(echo $IPFS | cut -c 24-);
    echo "$1-cli sendmany \"\" \"{\\";
    echo \\\"$(un DAx ipfs)\\\"":0.00001132,\\";
    echo \\\"$(un DBx "$3")\\\"":0.00001132,\\";
    while read LINE; do
        echo \\\"$(un DCx "$LINE")\\\"":0.00001000,\\";
    done;
    echo \\\"$(un DDx$FIRST)\\\"":0.00001000,\\";
    echo \\\"$(un DEx$LAST)\\\"":0.00001132}\""
}
